{
  "name": "amps",
  "version": "2.0.0",
  "type": "module",
  "private": true,
  "prisma": {
    "schema": "./prisma/schema"
  },
  "scripts": {
    "dev": "next dev",
    "dev:db": "docker compose --env-file=./.env -f ./docker/docker-compose.dev-db.yml up -d",
    "dev:email": "dotenvx run -- email dev --dir ./src/emails/messages -p 3001",
    "build:email": "dotenvx run -- email build --dir ./src/emails/messages",
    "start:email": "dotenvx run -- email start",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "lint:errs": "eslint --quiet",
    "tc": "tsc",
    "test": "vitest",
    "postinstall": "prisma generate --no-hints",
    "db:generate": "prisma generate",
    "db:generate-push": "pnpm run db:generate && pnpm run db:push",
    "db:studio": "prisma studio",
    "db:push": "eval $(grep '^DATABASE_URL' .env) && prisma db push",
    "db:drop": "eval $(grep '^DATABASE_URL' .env) && prisma migrate reset",
    "clearLogs": "rm *.log"
  },
